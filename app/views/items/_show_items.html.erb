<div class="container">
  <div class="row">
      <% items.each do |item| %>
    <div class="col-sm-12" style="background-color:tan;">
      <div class="thumbnail">
      <img style="max-height:17em" class="item_image" src=<%= item.image %>></img>
      <h4><%= item.title %></h4>
      <p><%= number_to_currency item.price %></p>
    </div>
    </div>
    <% end %>
  </div>
</div>
