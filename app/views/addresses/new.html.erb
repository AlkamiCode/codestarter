<div class="container">
<h3 id="title">Create Address</h3>
<%= form_for(@address, url: dashboard_path) do |f| %>
  <%= f.label :street_address %>
  <%= f.text_field :street_address %><br>

  <%= f.label :city %>
  <%= f.text_field :city %><br>

  <%= f.label :state %>
  <%= f.text_field :state %><br>

  <%= f.label :zip %>
  <%= f.text_field :zip %><br>

  <%= f.submit "Use Address" %>
  <% end %>
</div>
